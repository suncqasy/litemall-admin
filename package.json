{
  "name": "litemall-admin",
  "version": "1.0.0",
  "description": "litemall-admin basing on vue-element-admin 4.3.0",
  "author": "ysling <927069313@qq.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "build": "vue-cli-service build --mode production",
    "build:prod": "vue-cli-service build --mode production",
    "build:dep": "vue-cli-service build --mode deployment",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/assets/icons/svg --config=src/assets/icons/svgo.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "vue",
    "admin",
    "dashboard",
    "element-ui",
    "boilerplate",
    "admin-template",
    "management-system"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://gitee.com/JoyAtMeeting/litemall-plus"
  },
  "bugs": {
    "url": "https://github.com/linlinjava/litemall/issues"
  },
  "dependencies": {
    "@riophae/vue-treeselect": "0.4.0",
    "@tinymce/tinymce-vue": "3.0.1",
    "axios": "0.24.0",
    "canvas2image": "^1.0.5",
    "clipboard": "2.0.8",
    "connect": "3.6.6",
    "core-js": "3.19.1",
    "echarts": "4.9.0",
    "element-ui": "2.15.8",
    "file-saver": "2.0.5",
    "fuse.js": "6.4.3",
    "highlight.js": "9.18.5",
    "html2canvas": "^1.4.1",
    "js-beautify": "1.13.0",
    "js-cookie": "3.0.1",
    "jsencrypt": "3.0.0-rc.1",
    "lodash": "^4.17.11",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "quill": "1.3.7",
    "screenfull": "5.0.2",
    "script-loader": "0.7.2",
    "sortablejs": "1.10.2",
    "v-charts": "1.19.0",
    "vue": "2.6.12",
    "vue-count-to": "1.0.13",
    "vue-cropper": "0.5.5",
    "vue-meta": "2.4.0",
    "vue-router": "3.4.9",
    "vue-ueditor-wrap": "^2.5.6",
    "vuedraggable": "2.24.3",
    "vuex": "3.6.0",
    "xlsx": ">=0.17.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.4.6",
    "@vue/cli-plugin-eslint": "4.4.6",
    "@vue/cli-service": "4.4.6",
    "babel-eslint": "10.1.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "chalk": "4.1.0",
    "compression-webpack-plugin": "5.0.2",
    "connect": "3.6.6",
    "eslint": "7.15.0",
    "eslint-plugin-vue": "7.2.0",
    "lint-staged": "10.5.3",
    "runjs": "4.4.2",
    "sass": "1.32.13",
    "sass-loader": "10.1.1",
    "script-ext-html-webpack-plugin": "2.1.5",
    "svg-sprite-loader": "5.1.1",
    "vue-template-compiler": "2.6.12",
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@vue/cli-plugin-unit-jest": "3.5.3",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.5.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "chokidar": "2.1.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "serve-static": "^1.13.2",
    "svgo": "1.2.0"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
